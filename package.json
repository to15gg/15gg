{
  "name": "15gg",
  "version": "0.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start": "turbo run start",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "turbo": {
    "npmClient": "npm",
    "pipeline": {
      "start": {},
      "lint": {},
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "dist/**",
          ".next/**"
        ]
      },
      "dev": {
        "dependsOn": [
          "^build"
        ],
        "cache": false
      }
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix -c packages/eslint-preset-15gg/.eslintrc.js"
  },
  "dependencies": {
    "classnames": "^2.3.1"
  }
}
